{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "LocalProxyServer": "Information"
    }
  },
  "AllowedHosts": "*",
  "Proxy": {
    "Port": 8080,
    "UseHttps": true,
    "CrlPort": 8081,
    "LoadBalancingStrategy": "roundRobin",
    "Upstreams": [
      {
        "Enabled": true,
        "Type": "socks5",
        "Host": "localhost",
        "Port": 1081,
        "Process": {
          "AutoStart": true,
          "FileName": "ssh.exe",
          "Arguments": "root@proxy.dev -D 1081 -i dev.key",
          "WorkingDirectory": "%USERPROFILE%\\.ssh",
          "StartupDelayMs": 2000,
          "RedirectOutput": true,
          "AutoRestart": true,
          "MaxRestartAttempts": 5,
          "RestartDelayMs": 3000
        },
        "HealthCheck": {
          "Enabled": true,
          "IntervalMs": 30000,
          "TimeoutMs": 5000,
          "FailureThreshold": 3
        }
      },
      {
        "Enabled": false,
        "Type": "socks5",
        "Host": "localhost",
        "Port": 1082,
        "Process": {
          "AutoStart": true,
          "FileName": "ssh.exe",
          "Arguments": "root@proxy.dev -D 1082 -i dev.key",
          "WorkingDirectory": "%USERPROFILE%\\.ssh",
          "StartupDelayMs": 2000,
          "RedirectOutput": true,
          "AutoRestart": true,
          "MaxRestartAttempts": 5,
          "RestartDelayMs": 3000
        },
        "HealthCheck": {
          "Enabled": true,
          "IntervalMs": 30000,
          "TimeoutMs": 5000,
          "FailureThreshold": 3
        }
      }
    ]
  }
}